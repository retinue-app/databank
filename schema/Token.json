{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "https://schema.retinue.app/enums/Token.json",
  "title": "Token",

  "description": "Tokens that are part of the game.",

  "definitions": {
    "OfficialToken": {
      "anyOf": [
        { "$ref": "#/definitions/OfficialGreenToken" },
        { "$ref": "#/definitions/OfficialMiscToken" }
      ]
    },

    "OfficialGreenToken": {
      "enum": ["Aim", "Dodge", "Standby", "Surge"]
    },

    "OfficialMiscToken": {
      "enum": ["Suppression"]
    }
  }
}
