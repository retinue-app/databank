{
  "name": "@retinue/databank",
  "version": "0.0.0",
  "scripts": {
    "build:types": "json2ts --unknownAny --enableConstEnums --cwd schema -i schema/*.json -o src/types && prettier --ignore-path .gitignore --write src/types",
    "lint": "npm run lint:format && npm run lint:eslint && npm run lint:file-names",
    "lint:eslint": "eslint src",
    "lint:file-names": "ts-node ./tool/check/use-consistent-file-names.ts",
    "lint:format": "prettier --ignore-path .gitignore --list-different src"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/prettier": "^2.1.5",
    "eslint-plugin-import": "^2.22.1",
    "json-schema-to-typescript": "^9.1.1"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.4",
    "@types/glob": "^7.1.3",
    "@types/minimist": "^1.2.1",
    "@types/shelljs": "^0.8.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "ajv-cli": "^3.3.0",
    "cspell": "^4.2.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "minimist": "^1.2.5",
    "prettier": "^2.1.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
